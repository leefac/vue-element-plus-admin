{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "strict": true,
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "baseUrl": ".",
      "types": [
        "node",
      ]
    },
    "include": [
      "**/*.ts",
    ],
    "exclude": [
      "node_modules"
    ],
    "extends": "../tsconfig.json",
  }
  